package com.app.model;


import javax.persistence.*;
import lombok.*;

@Data
@NoArgsConstructor
@AllArgsConstructor
@Entity
public class Orders {
	
	@Id
	@GeneratedValue(strategy = GenerationType.AUTO)
	private Integer order_Id;
	private String product_Id;
	
//	@ManyToOne(cascade = CascadeType.ALL)
//	private Customer customers;
	
}
